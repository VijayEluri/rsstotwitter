<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" package="nz.gen.wellington.rsstotwitter.model">
    <class name="TwitterEvent" table="twitter_event">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="guid"/>   	
   		<property name="twit"/>
   		<property name="date"/>
   		<property name="publisher"/>   		   	
   		<many-to-one name="feed" class="Feed" column="feed" lazy="false" />
   		<many-to-one name="tweet" class="nz.gen.wellington.rsstotwitter.model.Tweet" column="tweet" cascade="" />
   		
	</class>
</hibernate-mapping>
