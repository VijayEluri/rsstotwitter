<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" package="nz.gen.wellington.rsstotwitter.model">
    <class name="FeedToTwitterJob" table="twittered_feed">
        <id name="id">
            <generator class="native" />
        </id>       
       	<many-to-one name="feed" class="nz.gen.wellington.rsstotwitter.model.Feed" column="feed" lazy="false" />        
   		<many-to-one name="account" class="nz.gen.wellington.rsstotwitter.model.TwitterAccount" column="account" lazy="false" />   		
   		<property name="tag" column="twitter_tag"/>
	</class>
</hibernate-mapping>
